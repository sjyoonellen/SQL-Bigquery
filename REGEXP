SELECT DISTINCT CITY FROM STATION WHERE CITY REGEXP '^[aeiouAEIOU]'

------------------------------------------------------------------------------

SELECT DISTINCT
    city
FROM
    STATION
WHERE
    LOWER(city) REGEXP '^[aeiou].*[aeiou]$'

------------------------------------------------------------------------------
SELECT DISTINCT
    CITY
FROM 
    STATION
WHERE 
    LOWER(LEFT(CITY, 1)) NOT REGEXP '^[aeiou]'
